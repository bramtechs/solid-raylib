{
  "name": "solid-raylib-monorepo",
  "version": "0.1.0",
  "type": "module",
  "description": "SolidJS renderer for Raylib - Monorepo",
  "private": true,
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build --workspace=solid-raylib-core",
    "start:demo": "npm run start --workspace=solid-raylib-demo"
